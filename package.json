    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "d3-geo": "^1.11.3",
    "node-sass": "^4.11.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router-dom": "^5.0.0",
    "topojson-client": "^3.0.0"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.3",
    "@storybook/addon-knobs": "^5.0.3",
